{
    "multipart": [
        {   "apply": { "model": "tubular:block/tube_center" }},
        {   "when": { "north": "true" },
            "apply": { "model": "tubular:block/tube_side" }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "tubular:block/tube_side", "y": 90 }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "tubular:block/tube_side", "y": 180 }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "tubular:block/tube_side", "y": 270 }
        },
        {   "when": { "up": "true" },
            "apply": { "model": "tubular:block/tube_side", "x": 270 }
        },
        {   "when": { "down": "true" },
            "apply": { "model": "tubular:block/tube_side", "x": 90 }
        },
        {   "when": { "facing": "up" },
            "apply": { "model": "tubular:block/tube_connector", "x": 270 }
        },
        {   "when": { "facing": "down" },
            "apply": { "model": "tubular:block/tube_connector", "x": 90 }
        },
        {   "when": { "facing": "east" },
            "apply": { "model": "tubular:block/tube_connector", "y": 90 }
        },
        {   "when": { "facing": "south"},
            "apply": { "model": "tubular:block/tube_connector", "y": 180 }
        },
        {   "when": { "facing": "west"},
            "apply": { "model": "tubular:block/tube_connector", "y": 270 }
        },
        {   "when": { "facing": "north"},
            "apply": { "model": "tubular:block/tube_connector" }
        }
    ]
}